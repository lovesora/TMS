{
  "packages": [
    "packages/*"
  ],
  "version": "0.1.1",
  "npmClient": "yarn",
  "npmClientArgs": [
    "--no-lockfile"
  ],
  "command": {
    "version": {
      "allowBranch": ["master", "release/*"],
      "conventionalCommits": true,
      "forcePublish": "*",
      "message": "chore(release): publish %s"
    }
  }
}
